<div class="row">
  {{#each model.suspects as |suspect|}}
    {{#if (eq suspect.name model.game.character.name)}}
      <div class="choose-character-container col-xs-2">
        <div class="choose-character" style="background-image: url({{suspect.image}});">
        </div>
      </div>
    {{/if}}
  {{/each}}
  <h1>Good evening, {{model.game.character.name}}.</h1>
</div>

{{turn-order model=model enterRoom='enterRoom' opponentGuess='opponentGuess' nextTurn='nextTurn'}}
